{
  "id": "com.wifipool",
  "name": {
    "en": "WiFi Pool"
  },
  "description": {
    "en": "WiFiPool integration with auto discovery of domain, sensors and switches."
  },
  "category": [
    "tools"
  ],
  "compatibility": ">=12.0.1",
  "sdk": 3,
  "brandColor": "#0b5ea8",
  "version": "1.2.0",
  "author": {
    "name": "Marco Ruiter"
  },
  "images": {
	"xlarge": "/assets/images/xlarge.png",
    "large": "/assets/images/large.png",
    "small": "/assets/images/small.png"
  },
  "permissions": ["homey:app:com.wifipool"],
  "api": {
    "autosetup": {
      "method": "POST",
      "path": "/autosetup"
    },
    "discover": {
      "method": "POST",
      "path": "/discover"
    },
    "test": {
      "method": "POST",
      "path": "/test"
    }
  }
}